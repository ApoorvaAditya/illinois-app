  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["-v"]
    },
    {
      "name": "Flutter-Dev",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["-v",
        "-t", "lib/mainDev.dart",
        "--flavor", "dev"
      ]
    },
    {
      "name": "Flutter-Prod",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["-v",
        "-t", "lib/mainProd.dart",
        "--flavor", "prod"
      ]
    },
    {
      "name": "Flutter-Test",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["-v",
        "-t", "lib/mainTest.dart",
        "--flavor", "test"
      ]
    }
  ]
}